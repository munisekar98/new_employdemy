<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    hideLabel="{Boolean}true"
    name="./customrte"
    xtype="richtext">
    <rtePlugins jcr:primaryType="nt:unstructured">
        <font
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <justify
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <undo
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <edit
          jcr:primaryType="nt:unstructured"
          defaultPasteMode="plaintext"
          features="[paste-plaintext]"
          stripHtmlTags="{Boolean}true"/>
        <findreplace
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <spellcheck
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <subsuperscript
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <misctools
            jcr:primaryType="nt:unstructured"
            features="*">
            <specialCharsConfig jcr:primaryType="nt:unstructured">
                <chars jcr:primaryType="nt:unstructured">
                    <copyright
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;copy;"/>
                    <registered
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;reg;"/>
                    <trademark
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;trade;"/>
                    <servicemark
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;#8480"/>
                    <emDash
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;#8212"/>
                    <asterisk
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;#8727"/>
                    <dagger
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;dagger;"/>
                    <double_dagger
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;Dagger;"/>
                    <upwardarrowname
                        jcr:primaryType="nt:unstructured"
                        entity="&amp;#8593"/>
                </chars>
            </specialCharsConfig>
        </misctools>
       <paraformat
            jcr:primaryType="nt:unstructured"
            features="*">
            <formats jcr:primaryType="cq:WidgetCollection">
                <heading1
                    jcr:primaryType="nt:unstructured"
                    description="Heading 1"
                    tag="h1"/>
                <heading2
                    jcr:primaryType="nt:unstructured"
                    description="Heading 2"
                    tag="h2"/>
                <heading3
                    jcr:primaryType="nt:unstructured"
                    description="Heading 3"
                    tag="h3"/>
                <heading4
                    jcr:primaryType="nt:unstructured"
                    description="Heading 4"
                    tag="h4"/>
                <heading5
                    jcr:primaryType="nt:unstructured"
                    description="Heading 5"
                    tag="h5"/>
                <heading6
                    jcr:primaryType="nt:unstructured"
                    description="Heading 6"
                    tag="h6"/>
                <paragraph
                    jcr:primaryType="nt:unstructured"
                    description="Paragraph"
                    tag="p"/>
                <div
                    jcr:primaryType="nt:unstructured"
                    description="Paragraph (no space)"
                    tag="div"/>
            </formats>
        </paraformat>
        <styles
            jcr:primaryType="nt:unstructured"
            features="*">
           <styles jcr:primaryType="cq:WidgetCollection">
                <greeting
                    jcr:primaryType="nt:unstructured"
                    cssName="greeting"
                    text="Greeting"/>
                <heading1
                    jcr:primaryType="nt:unstructured"
                    cssName="h1"
                    text="Heading 1"/>
                <heading2
                    jcr:primaryType="nt:unstructured"
                    cssName="h2"
                    text="Heading 2"/>
                <heading3
                    jcr:primaryType="nt:unstructured"
                    cssName="h3"
                    text="Heading 3"/>
                <heading4
                    jcr:primaryType="nt:unstructured"
                    cssName="h4"
                    text="Heading 4"/>
                <p1book
                    jcr:primaryType="nt:unstructured"
                    cssName="p1 -book"
                    text="P1 Book"/>
                <p1medium
                    jcr:primaryType="nt:unstructured"
                    cssName="p1 -medium"
                    text="P1 Medium"/>
                <p2book
                    jcr:primaryType="nt:unstructured"
                    cssName="p2 -book"
                    text="P2 Book"/>
                <p2medium
                    jcr:primaryType="nt:unstructured"
                    cssName="p2 -medium"
                    text="P2 Medium"/>
                <p3book
                    jcr:primaryType="nt:unstructured"
                    cssName="p3 -book"
                    text="P3 Book"/>
                <p3medium
                    jcr:primaryType="nt:unstructured"
                    cssName="p3 -medium"
                    text="P3 Medium"/>
                <p4book
                    jcr:primaryType="nt:unstructured"
                    cssName="p4 -book"
                    text="P4 Book"/>
                <p4medium
                    jcr:primaryType="nt:unstructured"
                    cssName="p4 -medium"
                    text="P4 Medium"/>
                <p5book
                    jcr:primaryType="nt:unstructured"
                    cssName="p5 -book"
                    text="P5 Book"/>
                <p5medium
                    jcr:primaryType="nt:unstructured"
                    cssName="p5 -medium"
                    text="P5 Medium"/>
                <style1light
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-1 -light"
                    text="Styling 1 - Light"/>
                <style1book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-1 -book"
                    text="Styling 1 - Book"/>
                <style2light
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-2 -light"
                    text="Styling 2 - Light"/>
                <style2book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-2 -book"
                    text="Styling 2 - Book"/>
               <style3light
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-3 -light" 
                    text="Styling 3 - Light"/>
                <style3book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-3 -book"
                    text="Styling 3 - Book"/>
                <style3medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-3 -medium"
                    text="Styling 3 - Medium"/>
                 <style4light
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-4 -light" 
                    text="Styling 4 - Light"/>
                 <style4book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-4 -book"
                    text="Styling 4 - Book"/>
                 <style4medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-4 -medium"
                    text="Styling 4 - Medium"/>
                 <style5book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-5 -book"
                    text="Styling 5 - Book"/>
                 <style5medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-5 -medium"
                    text="Styling 5 - Medium"/>              
                 <style6book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-6 -book"
                    text="Styling 6 - Book"/>
                 <style6medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-6 -medium"
                    text="Styling 6 - Medium"/>
                 <style7book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-7 -book"
                    text="Styling 7 - Book"/>
                 <style7medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-7 -medium"
                    text="Styling 7 - Medium"/>
                 <style8book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-8 -book"
                    text="Styling 8 - Book"/>
                 <style8medium
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-8 -medium"
                    text="Styling 8 - Medium"/>
                 <style9light
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-9 -light"
                    text="Styling 9 - Light"/>
                 <style9book
                    jcr:primaryType="nt:unstructured"
                    cssName="styling-9 -book"
                    text="Styling 9 - Book"/>                  
            </styles>
        </styles>
        <tool-tip
            jcr:primaryType="nt:unstructured"
            features="*"/>
        <color-picker
            jcr:primaryType="nt:unstructured"
            features="*">
            <colors jcr:primaryType="nt:unstructured">
                <Inky-blue
                    jcr:primaryType="nt:unstructured"
                    description="Inky-blue"
                    value="#0D1C3D"/>
                <dolphin
                    jcr:primaryType="nt:unstructured"
                    description="Dolphin"
                    value="#677083"/>
                <white
                    jcr:primaryType="nt:unstructured"
                    description="White"
                    value="#f7f7f7"/>
                <kp-blue
                    jcr:primaryType="nt:unstructured"
                    description="KP-blue"
                    value="#006BA6"/>
                <moss
                    jcr:primaryType="nt:unstructured"
                    description="Moss"
                    value="#4A7628"/>
            </colors>
        </color-picker>
        <image
            jcr:primaryType="nt:unstructured"
            features="*"/>
    </rtePlugins>
    <uiSettings jcr:primaryType="nt:unstructured">
	 <cui jcr:primaryType="nt:unstructured">
	  <inline jcr:primaryType="nt:unstructured" toolbar="[#format,#justify,#lists,links#modifylink,links#unlink,links#anchor,subsuperscript#subscript,subsuperscript#superscript,edit#paste-plaintext,findreplace#find,findreplace#replace,undo#undo,undo#redo,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,tool-tip#toolTip,#paraformat,color-picker#colorPicker,fullscreen#start,control#close,control#save,image#imageProps]">
			 <popovers jcr:primaryType="nt:unstructured"> 
			 <format jcr:primaryType="nt:unstructured" items="[format#bold,format#italic,format#underline]" ref="format"/>
			 <justify jcr:primaryType="nt:unstructured" items="[justify#justifyleft,justify#justifycenter,justify#justifyright]" ref="justify"/>
			 <lists jcr:primaryType="nt:unstructured" items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]" ref="lists"/>
			 <styles jcr:primaryType="nt:unstructured" items="styles:getStyles:styles-pulldown" ref="styles"/>
			 <paraformat jcr:primaryType="nt:unstructured" items="paraformat:getFormats:paraformat-pulldown" ref="paraformat"/> 
			 </popovers>
	   </inline>
	  <dialogFullScreen
			jcr:primaryType="nt:unstructured"
			toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,links#anchor,subsuperscript#subscript,subsuperscript#superscript,edit#paste-plaintext,findreplace#find,findreplace#replace,undo#undo,undo#redo,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,tool-tip#toolTip,#paraformat,color-picker#colorPicker]">
			<popovers jcr:primaryType="nt:unstructured">
				<styles jcr:primaryType="nt:unstructured"
					items="styles:getStyles:styles-pulldown" ref="styles" />
				<paraformat jcr:primaryType="nt:unstructured"
					items="paraformat:getFormats:paraformat-pulldown"
					ref="paraformat" />
			</popovers>
		</dialogFullScreen>
		<fullscreen
			jcr:primaryType="nt:unstructured"
			toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,links#anchor,subsuperscript#subscript,subsuperscript#superscript,edit#paste-plaintext,findreplace#find,findreplace#replace,undo#undo,undo#redo,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,tool-tip#toolTip,#paraformat,color-picker#colorPicker]">
			<popovers jcr:primaryType="nt:unstructured">
				<styles jcr:primaryType="nt:unstructured"
					items="styles:getStyles:styles-pulldown" ref="styles" />
				<paraformat jcr:primaryType="nt:unstructured"
					items="paraformat:getFormats:paraformat-pulldown"
					ref="paraformat" />
			</popovers>
		</fullscreen>
	</cui>
	</uiSettings>
    <htmlRules jcr:primaryType="nt:unstructured">
        <docType jcr:primaryType="nt:unstructured">
            <typeConfig jcr:primaryType="nt:unstructured">
                <semanticMarkupMap
                    jcr:primaryType="nt:unstructured"
                    b="strong"
                    i="i"/>
            </typeConfig>
        </docType>
        <blockHandling
            jcr:primaryType="nt:unstructured"
            removeSingleParagraphContainer="{Boolean}true"
            singleParagraphContainerReplacement="div"/>
        <links
            jcr:primaryType="nt:unstructured"
            protocols="[tel:,mailto:,ftp://,https://,http://,modal:]"/>
    </htmlRules>
</jcr:root>